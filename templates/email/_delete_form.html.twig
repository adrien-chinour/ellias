<form method="post" action="{{ path('email_delete', {'id': email.id}) }}"
      onsubmit="return confirm('T\'es sûr tu veux faire ça mec ?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ email.id) }}">
    <button class="btn btn-block btn-danger">Supprimer</button>
</form>
